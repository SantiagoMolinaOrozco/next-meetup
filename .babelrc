{
  "presets": ["next/babel"],
  "plugins": [
    ["import", { "libraryName": "antd" }],
    [
      "module-resolver",
      {
        "root": ["./src", "."]
      }
    ], [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ]
}
